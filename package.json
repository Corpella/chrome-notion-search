{
  "version": "2.1.1",
  "scripts": {
    "build": "pnpm run clean && vite build",
    "dev": "pnpm run clean && ENABLES_SENTRY=false vite",
    "visualize": "pnpm run clean && ENABLES_VISUALIZER=true vite build && open stats.html",
    "clean": "rimraf dist dist.zip",
    "test": "jest",
    "release": "pnpm install && pnpm run build && zip -r dist.zip dist",
    "lint:eslint": "eslint './{src,test}/**/*.{ts,tsx}'",
    "lint-fix:eslint": "eslint --fix './{src,test}/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check .",
    "lint-fix:prettier": "prettier --write .",
    "lint:stylelint": "stylelint './src/**/*.pcss'",
    "lint-fix:stylelint": "stylelint --fix './src/**/*.pcss'",
    "lint": "run-p -c lint:*",
    "lint-fix": "run-p -c lint-fix:*",
    "check:types": "tsc --noEmit",
    "check:ngword": "if git grep -rE \"console\\.(log|dir)|FIXME\" -- ':!package.json'; then exit 1; else echo \"All good\"; fi",
    "check": "run-p -c test lint check:*"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "https://gitpkg.now.sh/Cside/chrome-extension-tools/packages/vite-plugin?d137a66ae3175a003de5856dd6c4c3048347b2ca",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "^0.0.218",
    "@types/jest": "^29.4.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.escaperegexp": "^4.1.7",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.4.2",
    "jest-chrome": "^0.8.0",
    "jest-environment-jsdom": "^29.4.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^21.1.0",
    "lodash.debounce": "^4.0.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.1",
    "rimraf": "^4.1.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-csstree-validator": "^2.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-no-unsupported-browser-features": "^6.0.1",
    "stylelint-value-no-unknown-custom-properties": "^4.0.0",
    "throttle-debounce": "https://github.com/Cside/throttle-debounce",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.x",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@sentry/integrations": "^7.39.0",
    "@sentry/react": "^7.39.0",
    "@sentry/tracing": "^7.39.0",
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.7",
    "compare-versions": "^6.0.0-rc.1",
    "extensible-custom-error": "^0.0.7",
    "lodash.escaperegexp": "^4.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-string-replace": "^1.1.0",
    "react-tooltip": "^5.7.5",
    "use-query-params": "^2.1.2"
